# Copy to .env and set values
OPENAI_API_KEY=
# Optional override (defaults to gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# Langfuse telemetry configuration
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=

# Capture chain-of-thought only in Langfuse (never displayed to users)
# Accepts: true/false, 1/0, yes/no, on/off
LANGFUSE_CAPTURE_COT=false
